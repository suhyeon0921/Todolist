generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          Int     @id @default(autoincrement())
  email       String? @unique @map("email")
  phoneNumber String? @unique @map("phone_number")
  password    String
  fullName    String  @map("full_name")
  nickname    String  @unique
}
